#nombre
class Orden_trabajo:

#atributos
  def __init___(self, nombre, tipo, ubicacion, relevancia):

    self.nombre = nombre
    self.tipo = tipo
    self.ubicacion = ubicacion
    self.relevancia = relevancia

#metodos
  def proteger(self):
    print(f"{self.nombre} proteger el area a intervenir")
  def realizacion_de_trabajo(self):
    print(f"{self.nombre} realizar el trabajo requerido por el cliente")
  def llenado_de_documentos(self):
    print(f"{self.nombre} llenar satisfaccion del cliente y hoja de servicio")


